/* CENTER PANEL:RESULT PANEL: APPRIS Panel */
#appris_panel {
	padding: 10px 20px 10px 20px;
}
#appris_panel .title {
	font-family: "Lucida Sans Unicode", sans-serif, Verdana;
	font-size: 18px;	
	font-weight: bold;
	display: inline;
}
#appris_panel .report table.appris_panel {
	border-spacing: 1px;
	border-width: 1px;
	border-style: solid;
	border-collapse: separate;
	border-color: #000000;
	background-color: white;
	font-family: "Lucida Sans Unicode", sans-serif, Verdana;
	font-size: 11px;	
	cursor: default;
	margin-top: 10px;
}
#appris_panel .report th {
	padding-left: 3px;
	padding-right: 3px;
	border-width: 1px;
	border-style: solid;
	border-color: #000000; 
	-moz-border-radius: 3px 3px 3px 3px;
	background-color: #F4A927;
	text-align: left;
	vertical-align: top;
	font-family: "Lucida Sans Unicode", sans-serif, Verdana;
	font-size: 11px;
	font-weight: bold;
	width: auto;
}
#appris_panel .report th .label{
	width: auto;
	height: auto;
	margin: 0;
	padding: 0;
	vertical-align: bottom;
}
#appris_panel .report td {
	border-width: 1px;
	border-style: solid;
	border-color: #000000;
	background-color: white;
	text-align: center;
}
#appris_panel .report td.pi {
	background-color: #FFF3B3;
}
#appris_panel .report th.header { 
	background-image: url('../img/small.gif');     
	cursor: pointer; 
	font-weight: bold; 
	background-repeat: no-repeat; 
	background-position: center right; 
	padding-left: 5px; 
	border-right: 1px solid #DAD9C7; 
	margin-left: -1px;
	z-index: 1; 
}
#appris_panel .report th.headerSortUp { 
	background-image: url('../img/small_asc.gif'); 
	background-color: #D29123; 
}
#appris_panel .report th.headerSortDown { 
	background-image: url('../img/small_desc.gif'); 
	background-color: #D29123; 
}
#appris_panel .report .OK {
	background-image: url('../img/YES-icon.png');
	background-repeat: no-repeat;
	width: 10px;
	height: 12px;
	margin-left: auto;
	margin-right: auto;
}
#appris_panel .report .pop_external_ref {
	cursor: pointer;
	font-weight: bold;
}
#appris_panel .report span.pop_external_ref {
	background: transparent url(../img/external2.png) no-repeat scroll right center;
	float: right;
}

/* CENTER PANEL:RESULT PANEL: UCSC Panel */
#ucsc_appris_panel,
#ucsc_proteo_panel,
#ucsc_rnaseq_panel {
	padding: 10px 20px 10px 20px; 
}
#ucsc_appris_panel .title,
#ucsc_proteo_panel .title,
#ucsc_rnaseq_panel .title {
	font-family: "Lucida Sans Unicode", sans-serif, Verdana;
	font-size: 18px;
	font-weight: bold;
	display: inline;
}
#ucsc_appris_panel .report table.ucsc_panel,
#ucsc_proteo_panel .report table.ucsc_panel,
#ucsc_rnaseq_panel .report table.ucsc_panel {
	text-align: center;
	cursor: default;
	margin-top: 10px;
	border-width: 1px;
	border-style: solid;
	border-color: #000000; 
	-moz-border-radius: 3px 3px 3px 3px;
	background-color: #F4A927;
}
#ucsc_appris_panel .report table.ucsc_panel td,
#ucsc_proteo_panel .report table.ucsc_panel td,
#ucsc_rnaseq_panel .report table.ucsc_panel td {
	margin: 0;
    padding: 1px 1px 1px 1px;
	width: 800px;
	font-family: "Lucida Sans Unicode", sans-serif, Verdana;
	font-size: 11px;	
	font-weight: bold;
	text-align: center;
}
#ucsc_appris_panel .report table.ucsc_panel table.ucsc_img_data,
#ucsc_proteo_panel .report table.ucsc_panel table.ucsc_img_data,
#ucsc_rnaseq_panel .report table.ucsc_panel table.ucsc_img_data {
	background-color: #FFFFFF;
	border: solid;
	border-width: 1px;
	border-color: black;
    width: 100%;
	height: 100%;
	margin: 0;
	padding: 0;
	cursor: pointer;
}
#ucsc_appris_panel .report table.ucsc_panel table.ucsc_img_head,
#ucsc_proteo_panel .report table.ucsc_panel table.ucsc_img_head,
#ucsc_rnaseq_panel .report table.ucsc_panel table.ucsc_img_head {
	background-color: #FFFFFF;
	border: solid;
	border-width: 1px;
	border-color: black;
    width: 100%;
	height: 100%;
	margin: 0;
	padding: 0;
}
#ucsc_appris_panel .report table.ucsc_panel table.ucsc_img_footer,
#ucsc_proteo_panel .report table.ucsc_panel table.ucsc_img_footer,
#ucsc_rnaseq_panel .report table.ucsc_panel table.ucsc_img_footer {
	background-color: #FFFFFF;
	border: solid;
	border-width: 1px;
	border-color: black;
    width: 100%;
	height: 100%;
	margin: 0;
	padding: 0;
	cursor: pointer;
}


/* CENTER PANEL:RESULT PANEL: Sequence Panel */
#sequence_panel {
	padding: 10px 20px 10px 20px; 
}
#sequence_panel .title {
	font-family: "Lucida Sans Unicode", sans-serif, Verdana;
	font-size: 18px;
	font-weight: bold;
}
#sequence_panel .report table {
	text-align: left;
	cursor: default;
	margin-top: 10px;
	/* width: 1010px; */
}
#sequence_panel .report td {
	text-align: center;
	margin: 0;
	padding: 0;
}
#sequence_panel .report th {
	padding: 3px 5px 3px 3px;
	border-width: 1px;
	border-style: solid;
	border-color: #000000; 
	-moz-border-radius: 3px 3px 3px 3px;
	background-color: #F4A927;
	text-align: left;	
	font-family: "Lucida Sans Unicode", sans-serif, Verdana;
	font-size: 11px;
	font-weight: bold;
	width: auto;
}
#sequence_panel .report td.title {
	padding: 3px 5px 3px 3px;
	font-family: "Lucida Sans Unicode", sans-serif, Verdana;
	font-size: 11px;
	font-weight: bold;
	width: auto;
}
#sequence_panel .report td.title span.title_seq {
	text-align: left;
	float: left;
}
#sequence_panel .report td.title div.show_met {
	font-weight: normal;
	text-align: left;
	float: right;
}
#sequence_panel .report td.title div.show_seq {
	float: right;
	font-weight: normal;
}
#sequence_panel .report tr.sequence td.img_seq {
	width: 700px;
	vertical-align: top;
}
#sequence_panel .report td.show_methods {
	vertical-align: top;
}
#sequence_panel .report td.show_methods table.show_methods {
	margin: 5px 0 10px 0;
	width: 290px;
}
#sequence_panel .report td.show_methods .show_methods .demo {
	margin: 0 10px 0 10px;
	font-size: 14px;
}
#sequence_panel .report td.show_methods .show_methods .desc {
	padding: 0 20px 0 5px;
	text-align: left;
}
#sequence_panel .report td.show_methods .show_methods .functional_residue .demo {
	background-image: url('../img/firestar-demo.png');
	background-repeat: no-repeat;
	width: 11px;
	height: 11px;
}
#sequence_panel .report td.show_methods .show_methods .homologous_structure .demo {
	background-image: url('../img/matador3d-demo.png');
	background-repeat: no-repeat;
	width: 11px;
	height: 10px;
}
#sequence_panel .report td.show_methods .show_methods .functional_domain .demo {
	background-image: url('../img/spade-demo.png');
	background-repeat: no-repeat;
	width: 11px;
	height: 11px;
}
#sequence_panel .report td.show_methods .show_methods .transmembrane_signal .demo {
	background-image: url('../img/thump-demo.png');
	background-repeat: no-repeat;
	width: 11px;
	height: 11px;
}
#sequence_panel .report td.show_methods .show_methods .peptide_mitochondrial_signal .demo {
	background-image: url('../img/crash-demo.png');
	background-repeat: no-repeat;
	width: 11px;
	height: 11px;
}
#sequence_panel .report td.title .show_methods_btn {
	width: 290px;
}
#sequence_panel .report td.title .show_methods_btn input {
	float: right;
}
#sequence_panel .report div.seq {
	float: left;
}
#sequence_panel .report div.seq span {
	font-family: "Courier New", sans-serif, Verdana;
	font-size: 14px;
	text-align: left;
	letter-spacing: 1px;
	line-height: 20px;
	margin: 0 0 20px 52px;
	display: inline-table;
}
#sequence_panel .report div.seq svg {
	float: left;
	margin-left: 10px;
}
#sequence_panel .report div.seq .functional_residue, 
#sequence_panel .report div.seq .functional_domain,
#sequence_panel .report div.seq .transmembrane_signal, 
#sequence_panel .report div.seq .peptide_mitochondrial_signal {
	display: block;
}
#sequence_panel .report div.img_seq .homologous_structure {
	fill: #800000;	
}



/* CENTER PANEL:RESULT PANEL: APPRIS Panel */
#log_panel {
	position: relative;
	top: 0;
	left: 0;
	bottom: 0;
	right: 0;
	margin: 0;
	padding: 0px 20px 0px 20px;
	width: auto;
	height: auto;
	overflow: auto;
}
#log_panel a {
	font-weight: bold;
	font-size: 13px;
	color: #D29123;
	text-decoration: underline;
	cursor: pointer;
}


/*
 *
 * BOTTOM PANEL
 *
 */
#bottom_panel {
	bottom: 0;
	left: 0;
	margin: 0;
	padding: 10px 30px 10px 10px;
	width: auto;
	height: auto;	
	color: black;
	font-family: "Lucida Sans Unicode", normal, sans-serif, Verdana;
	font-size: 11px;
	text-align: right;
}
#bottom_panel a {
	color: blue;
	text-decoration: underline;
	cursor: pointer;
}


