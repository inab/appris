/*
 *
 * MAIN PANELS
 * 
 */
body {
	margin: 0;
	padding: 0;
	background-color: white;
	font-family: "Lucida Sans Unicode", sans-serif, Verdana;
	font-size: 12px;
	cursor: default;
}
table {
	font-family: "Lucida Sans Unicode", sans-serif, Verdana;
	font-size: 11px; 
	cursor: default;	
}
td {
	text-align: left;
}
th {
	font-family: "Lucida Sans Unicode", sans-serif, Verdana;
	font-size: 12px;
	text-align: center;
}

/*
 *
 * TOP PANEL
 *
 */
#top_panel {
	position: absolute;
	top: 0;
	left: 0;
	right: 0;
	bottom: 0;
	margin: 0;
	padding: 0;
	width: 100%;
	height: 32px;
	background-color: #F4A927;
	color: black;
	font-family: "Lucida Sans Unicode", sans-serif, Verdana;
	font-size: 11px;	
}
/* TOP PANEL: Log frame */
#logo_frame {
	position: absolute;
	top: 4px;
	left: 5px;
	padding: 0;
	text-align: center;
	background-image: url('../img/APPRIS-icon.png');
	background-repeat: no-repeat;
	width: 214px;
	height: 20px;
	cursor: pointer;
	z-index: 2;
}
/* TOP PANEL: Search frame */
#search_frame {
	position: absolute;
	top: 4px;
	right: 10px;
	z-index: 1;
}
#search_frame_launch {
	margin-right: 25px;
}
#search_frame_launch input {
	margin: 0;
	padding: 0;
	height: 18px;
	width: 200px;
}
#search_frame_image {
	position: absolute;
	top: 3px;
	right: 3px;
	width: auto;
	height: auto;
	cursor: pointer;
	background-image: url('../img/isearch.png');
	width: 18px;
	height: 18px;
}

/*
 *
 * MENU PANEL
 *
 */
#menu_panel {
	position: absolute;
	top: 32px;
	left: 0;
	right: 0;
	bottom: 0;
	margin: 0;
	padding: 0;
	width: 100%;
	height: 20px;
	background-color: #FFF3B3;
	color: black;
	font-family: "Lucida Sans Unicode", sans-serif, Verdana;
	font-size: 11px;	
}
/* MENU PANEL: Menu frame */
#menu_frame {
	position: relative;
	top: 0;
	bottom: 0;
	margin:0;
	padding-left: 5px;
	width: auto;
	height: 20px;
}	
#menu_frame a {
	cursor: pointer;
	color: black;
}
/* MENU PANEL: Info frame */
#info_frame {
	position: absolute;
	top: 0;
	right: 0;
	width: auto;
	height: auto;
	margin: 0;
	padding-right: 10px;	
}
#info_frame a {
	cursor: pointer;	
	text-decoration: none;
	color: black;
}
#info_frame a:hover {
	color: #909090;
}

/*
 *
 * HEADER PANEL
 *
 */
#header_panel {
	position: absolute;
	top: 52px;
	left: 0;
	right: 0;
	bottom: 0;
	margin: 0;
	padding: 0;	
	width: 100%;
	height: 50px;
	color: black;
	font-family: "Lucida Sans Unicode", sans-serif, Verdana;
	font-size: 12px;	
}
/* HEADER PANEL: Status frame */
#header_panel .status_panel {
	position: absolute;
	top: 0;
	bottom: 0;
	width: auto;
	height: 40px;
}
#header_panel .status_panel .loading_frame {
	position: relative;
	float: left;
	margin-left: 10px;
	margin-top: 5px;
	background-image: url('../img/loading.gif');
	background-repeat: no-repeat;
	width: 32px;
	height: 32px;
	cursor: default;
	display: none;
}
#header_panel .status_panel .status_message {
	margin-left: 50px;
	padding-top: 10px;
	display: none;
}
/* HEADER PANEL: Logo frame */
#logo_panel {
	position: absolute; 
	top: 10px;
	bottom: 0;
	width: 100%;
}
#logo_panel .logo_title {
	color: #17192e;
	font-family: "Lucida Sans Unicode", sans-serif, Verdana;
	font-weight: bold;
	font-size: 20px;
	text-align: center;
	display: block;
	margin-left: auto;
	margin-right: auto;
	padding: 0;
	width: 800px;
}
#logo_panel .logo_title_appris {
	position: relative;
	margin-left: auto;
	margin-right: auto;
	background-image: url('../img/APPRIS-title-icon.png');
	background-repeat: no-repeat;
	width: 930px;
	height: 18px;
	cursor: default;
}

/*
 * 
 * CENTER PANEL
 *
 */
#center_panel {
	position: absolute;
	top: 102px;
	left: 0;
	right: 0;
	bottom: 0;
	margin: 0;
	padding: 0;	
	width: 100%;
	height: auto;
}
/* CENTER PANEL: Search frame */
#search_frame2 {
	font-family: "Lucida Sans Unicode", sans-serif, Verdana;
	font-weight: normal;
	font-size: 12px;	
	position: relative;	
	text-align: center;
	margin-left: auto;
	margin-right: auto;
	width: 420px;
	height: auto;
}
#search_frame_launch2 span {
	float: left;
	margin-right: 3px;
	padding: 0;
}
#search_frame_launch2 input.run_txt {
	float: left;
	margin-right: 3px;
	padding: 0;
	height: 18px;
	width: 200px;
}
#search_frame_launch2 input.run_btn {
	float: left;
	padding: 1px;
	background: #aaa;
	color: #fff;
	text-decoration: none;
	display: block;
	cursor: pointer;
	width: 28px;
	height: 22px;
	font-weight: bold;
}


/*
 * 
 * CONTENT PANEL
 *
 */
#content_panel {
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	width: auto;
	height: auto;
	margin: 0;
	padding: 0px 30px 20px 30px;
	text-align: justify;
	font-family: "Lucida Sans Unicode", sans-serif, Verdana;
	font-weight: normal;
	font-size: 12px;
}
#content_panel a {
	color: blue;
}
#content_panel img {
	margin: 5px 5px 5px 15px;
	border-style: none; 
}
#content_panel .content_leftcol {
	float: left;
	margin-bottom: 20px;
	width: 50%;
	height: auto;
}
#content_panel .content_rightcol {
	float: right;
	margin-bottom: 20px;
	width: 50%;
	height: auto;
}
#content_panel .content_leftcol_first {	
	height: 40px;
	width: auto;
	padding: 15px 15px 15px 15px;
	background-color: #FFF3B3;
	border: 3px double #EEE;
	border-color: #E4E4E4 #CFCFCF #CCC #CFCFCF;
}
#content_panel .content_leftcol_first, .content_leftcol_second, .content_leftcol_third {
	margin-right: 20px;
	margin-bottom: 20px;
}
#content_panel .content_rightcol_first, .content_rightcol_second, .content_rightcol_third {
	margin-bottom: 20px;
}
#content_panel .content_rightcol_first {
	text-decoration: none;
}
#content_panel .content_leftcol_second, .content_leftcol_third, .content_rightcol_first, .content_rightcol_second, .content_rightcol_third {	
	padding: 15px 15px 15px 15px;
	border: 3px double #EEE;
	border-color: #E4E4E4 #CFCFCF #CCC #CFCFCF;
}
#content_panel .species-box {
	position: relative;
	margin-bottom: 10px;
}
#content_panel .species-box img {
	width: 62px;
	border-radius: 5px 5px 5px 5px;
	-moz-border-radius: 5px 5px 5px 5px;
}
#content_panel .species-box span {
	display: inline-block;
	vertical-align: top;
}


#content_panel .content_title {
	color: #17192e;
	font-family: "Lucida Sans Unicode", sans-serif, Verdana;
	font-size: 20px;
	text-align: center;
	display: block;
	margin-left: auto;
	margin-right: auto;
	padding: 0px 0px 5px 0px;
	width: 500px;	
}
#content_panel .content_subtitle {
	color: black;
	font-family: "Lucida Sans Unicode", sans-serif, Verdana;
	font-weight: bold;
	font-size: 18px;
	padding: 30px 0px 5px 0px;
}
#content_panel .content_subtitle2 {
	color: black;
	font-family: "Lucida Sans Unicode", sans-serif, Verdana;
	font-weight: bold;
	font-size: 14px;
	padding: 10px 0px 5px 0px;
}
#content_panel .content_paragrah {
	margin-bottom: 15px;
}
#content_panel .content_highlighting {
	font-weight: bold;
}
#content_panel .content_footer {
	margin-top: 275px;
	font-family: "Lucida Sans Unicode", sans-serif, Verdana;
	font-weight: normal;
	font-size: 10px;
}
#content_panel .content_footer a {
	color: blue;
}
#content_panel .detecting_principal_isoform_image {
	background: transparent url(../img/detecting_principal_isoform.png) no-repeat scroll right center;	
	margin: 0;
	top: 0;
}
#content_panel .content_external_ref {
	background: transparent url(../img/external.png) no-repeat scroll right center;
	padding-right: 13px;
}
#content_panel .title_method {
	margin-top: 50px;
	margin-bottom: 20px;
}
#content_panel .logo_title_firestar, a.logo_title_firestar{
	color: #D29123; /*#9f1300;*/
	font-weight: bold;
}
#content_panel .logo_image_firestar {
	background: transparent url(../img/firestar_logo_small.png) no-repeat scroll right center;
	width: 80px;
	height: 72px;
	float: left;
	margin-right: 15px;
	margin-bottom: 10px;
}
#content_panel .logo_title_matador3d, a.logo_title_matador3d {
	color: #C60806; /*#f6b900;*/
	font-weight: bold;
}
#content_panel .logo_title_spade, a.logo_title_spade {
	color: #769C02;
	font-weight: bold;
}
#content_panel .logo_image_spade {
	background: transparent url(../img/pfamscan_logo_small.png) no-repeat scroll right center;
	width: 150px;
	height: 54px;
	float: left;
	margin-right: 15px;
	margin-bottom: 10px;
}
#content_panel .logo_title_inertia, a.logo_title_inertia {
    color: #705C9F; /*#004080;*/
	font-weight: bold;
}
#content_panel .logo_title_crash, a.logo_title_crash {
	color: #755B2D; /*#6b8538;*/
	font-weight: bold;
}
#content_panel .logo_title_thump, a.logo_title_thump {
	color: #004142;
	font-weight: bold;
}
#content_panel .logo_title_cexonic, a.logo_title_cexonic {
	color: #6787C8; /*#6b85d0;*/
	font-weight: bold;
}
#content_panel .logo_image_cexonic {
	background: transparent url(../img/cexonic_logo_small.png) no-repeat scroll right center;
	width: 150px;
	height: 43px;
	float: left;
	margin-right: 15px;
	margin-bottom: 10px;	
}
#content_panel .logo_title_corsair, a.logo_title_corsair {
	color: #2C885B;
	font-weight: bold;
}
#content_panel .report table.reference {
	margin: 0;
	border-spacing: 1px;
	border-width: 1px;
	border-style: solid;
	border-collapse: separate;
	border-color: #000000;
	background-color: white;
	font-family: "Lucida Sans Unicode", sans-serif, Verdana;
	font-size: 11px;	
	cursor: default;
	text-align: center;
	margin-top: 10px;
}
#content_panel .report tr.reference {
	height: 30px;
}
#content_panel .report td.reference {
	padding: 3px 3px 3px 3px;
	border-width: 1px;
	border-style: solid;
	border-color: #000000;
	background-color: white;
	text-align: center;
}
#content_panel .report th {
	padding: 3px 3px 3px 3px;
	border-width: 1px;
	border-style: solid;
	border-color: #000000; 
	-moz-border-radius: 3px 3px 3px 3px;
	background-color: #F4A927;
	font-family: "Lucida Sans Unicode", sans-serif, Verdana;
	font-size: 11px;	
	font-weight: bold;
	width: 90px;
	text-align: left;
}
#popup_info {
	width: auto;
	margin: 0;
	padding: 0;
	text-align: right;
	z-index: 2;
}					
#sPopup-container{
	z-index: 9000;
	position: fixed;
	left: 0;
	right: 0;
	top: 0;
	background: url('../img/popup/bg.png');
	background-color: transparent;
	width: 100%;
	height:100%;
	text-align: left;
}
#sPopup-popup{
	font-family: "Lucida Sans Unicode", sans-serif, Verdana;
	font-size: 12px;
	font-weight: normal;	
	width: 800px;
	height: 90%;
	overflow: auto;
	margin: 25px auto;
	background: #fff;
	padding: 10px;
	border: 1px solid #999;
}
#sPopup-close{
	position: fixed;
	margin-bottom: 15px;
	margin-left: 770px;
	width: 18px;
	height: 18px;
	display: block;
	background: url('../img/popup/closehover.png');
	cursor: pointer;
}
#sPopup-close:hover{
	background: url('../img/popup/close.png');
}
.sPopup-content{
	display: none;
	background: #ccc;
}


/*
 *
 * SEARCH PANEL
 *
 */
#search_panel .title {
	font-family: bold, "Lucida Sans Unicode", sans-serif, Verdana;
	font-size: 16px;
	font-weight: bold;
	color: #17192E;
}
#search_panel {
	position: relative;
	top: 0;
	left: 0;
	bottom: 0;
	right: 0;
	margin: 0;
	padding: 0px 20px 0px 20px;
	width: auto;
	height: auto;
	overflow: auto;
}
#search_panel a {
	font-weight: bold;
	font-size: 13px;
	color: #D29123;
	text-decoration: underline;
	cursor: pointer;
}
#search_panel a:hover {
	color: #F4A927;	
}
#search_panel .head {
	padding-top: 10px;
}
#search_panel .arrow { 
	float: left;
	background-image: url('../img/arrow_right.png');
	background-repeat: no-repeat;
	width: 14px;
	height: 14px;
	cursor: pointer;
	padding-left: 7px;
	margin-top: 3px;
}
#search_panel .head .num_searches {
	font-weight: bold;
	text-decoration: underline;
	cursor: pointer;
}
#search_panel .head .num_species {
	font-weight: bold;
}
#search_panel .content {
	padding: 0px 10px 10px 30px;
}
#search_panel .report {
	padding: 10px 10px 10px 30px;
	display: none;
}
#search_panel .report p {
	margin: 0;
	padding: 0 10px 20px 10px;
}
#search_panel .report .subtitle {
	font-family: bold, "Lucida Sans Unicode", sans-serif, Verdana;
	font-size: 12px;
	font-weight: bold;
	color: #17192E;
}
#search_panel .report .subtitle2, #search_panel .report .subtitle2 a {
	font-family: normal, "Lucida Sans Unicode", sans-serif, Verdana;
	font-size: 12px;
	font-weight: normal;
	color: #17192E;
}


/*
 *
 * RESULT PANEL
 *
 */
#result_panel {
	position: relative;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	margin: 0;
	padding: 0;	
	width: auto;
	height: auto;
}
#result_panel .menu {
	text-align: left;
	margin-left: 10px;
	color: black;
	display: inline;
}
#result_panel .popup_info_img, .popup_info_img { 
	width: auto;
	height: 17px;
	margin: 0 !important;
	padding: 3px 3px 3px 3px;
	right: 0;
	display: inline;
	z-index: 2;
	cursor: pointer;
}
#result_panel .status_panel {
	margin-left: auto;
	margin-right: auto;
	width: 80px;
}	
#result_panel .status_panel .loading_frame {
	float: left;
	background-image: url('../img/loading.gif');
	background-repeat: no-repeat;
	width: 32px;
	height: 32px;
	cursor: default;
	display: none;
}
#result_panel .status_panel .status_message {
	margin-left: 40px;
	padding-top: 5px;
	display: none;
}

#result_panel .export_menu, #result_panel .tool_menu {
	position: relative;
	top: 0;
	float: left;
	padding: 0;
	margin: 0;	
	width: auto;
	height: 0;
	list-style: none;
}
#result_panel .export_menu ul, #result_panel .tool_menu ul {
	padding: 3px 10px 3px 5px;
	list-style: none;
	background-color: #FFF3B3;
	border: 2px solid #EEE;
	border-color: #E4E4E4 #CFCFCF #CCC #CFCFCF;
	text-align: left;
}
#result_panel .export_menu a, #result_panel .tool_menu a {
	text-decoration: underline;
	cursor: pointer;
	color: black;
}
#result_panel .export_menu a:hover, #result_panel .tool_menu a:hover {
	color: #909090;
}
#result_panel .export_menu .close, #result_panel .tool_menu .close {
	float: right;
	width: 14px;
	height: 14px;
	display: block;
	background: url('../img/export_menu-closehover.png');
	cursor: pointer;
}
#result_panel .export_menu .close:hover, #result_panel .tool_menu .close:hover {
	background: url('../img/export_menu-close.png');
}

/* CENTER PANEL:RESULT PANEL: Title panel */
#title_panel {
	padding: 10px 10px 10px 10px;
}
#title_panel .report {
	position: relative;
	top: 0;
	left: 0;
	bottom: 0;
	right: 0;
	padding: 0;
	width: auto;
	height: 100px;
}
#title_panel .report .title {
	font-family: bold, "Lucida Sans Unicode", sans-serif, Verdana;
	font-size: 14px;
	font-weight: bold;
	color: #D29123;
}

#title_panel .report .subtitle {
	font-family: bold, "Lucida Sans Unicode", sans-serif, Verdana;
	font-size: 12px;
	font-weight: bold;
	color: #17192E;	
	width: auto;
	cursor: default;
}
#title_panel .report .subtitle a {
	text-decoration: underline;
	cursor: pointer;
}
#title_panel .report .subtitle:hover {
	color: black;
}

