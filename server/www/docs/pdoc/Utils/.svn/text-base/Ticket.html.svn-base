			<span id="perlKey">my</span> <span id="braces">(</span><span id="var">$seq_id</span><span id="braces">)</span> = <span id="var">$seqObj</span>-&gt;<span id="method">id</span>;
			<span id="perlKey">my</span> <span id="braces">(</span><span id="var">$seq</span><span id="braces">)</span> = <span id="var">$seqObj</span>-&gt;<span id="method">seq</span>;			
			<span id="var">$data</span> .= <span id="var">$seq_id</span>.<span id="sqStr">':'</span>.<span id="var">$seq</span>.<span id="sqStr">'|'</span>;
		<span id="braces">}</span>
		<span id="var">$data</span> =~ <span id="regExp">s/\|$//</span>;
	<span id="braces">}</span>;
	<span id="method">throw</span><span id="braces">(</span><span id="sqStr">'Argument must be a correct fasta file'</span><span id="braces">)</span> <span id="perlKey">if</span> <span id="braces">(</span>$@<span id="braces">)</span>;
	
	<span id="comment"># create md5</span><br />	<span id="perlKey">eval</span> <span id="braces">{</span>
		<span id="perlKey">my</span> <span id="braces">(</span><span id="var">$ctx</span><span id="braces">)</span> = <span id="module">Digest::MD5</span>-&gt;<span id="perlKey">new</span>;
		<span id="var">$ctx</span>-&gt;<span id="method">add</span><span id="braces">(</span><span id="var">$data</span><span id="braces">)</span>;
		<span id="var">$ticket_id</span> = <span id="var">$ctx</span>-&gt;<span id="method">hexdigest</span>;		
	<span id="braces">}</span>;
	<span id="method">throw</span><span id="braces">(</span><span id="sqStr">'Creating md5'</span><span id="braces">)</span> <span id="perlKey">if</span> <span id="braces">(</span>$@<span id="braces">)</span>;
	
	<span id="var">$self</span>-&gt;<span id="braces">{</span><span id="sqStr">'ticket'</span><span id="braces">}</span> = <span id="var">$ticket_id</span>;
	<span id="perlKey">return</span> <span id="var">$ticket_id</span>;</pre>}</td></div>
